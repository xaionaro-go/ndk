<?xml version="1.0" encoding="utf-8"?><manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="FyneCameraDemo.dx.streampanel"
	android:versionCode="6"
	android:versionName="0.1.0">

	<application android:label="FyneCameraDemo" android:debuggable="true">
	<activity android:name="org.golang.app.GoNativeActivity"
		android:label="FyneCameraDemo"
		android:configChanges="orientation|screenSize|smallestScreenSize|screenLayout|keyboardHidden|uiMode"
		android:exported="true"
		android:theme="@android:style/Theme"
		android:windowSoftInputMode="adjustResize">
		<meta-data android:name="android.app.lib_name" android:value="FyneCameraDemo" />
		<intent-filter>
			<action android:name="android.intent.action.MAIN" />
			<category android:name="android.intent.category.LAUNCHER" />
		</intent-filter>
	</activity>
	</application>

	<uses-permission android:name="android.permission.CAMERA" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.INTERNET" />
</manifest>